<?xml version="1.0" encoding="utf-8"?>
<Map srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" bgcolor="#ffffff">
    <Style name="raster_style">
        <Rule>
            <RasterSymbolizer/>
        </Rule>
    </Style>
    <Style name="states_outlines">
        <Rule>
            <LineSymbolizer>
                <CssParameter name="stroke-width">0.3</CssParameter>
            </LineSymbolizer>
        </Rule>
    </Style>
    <Style name="states_pg_labels">
        <Rule>
            <TextSymbolizer name="state_abbr" face_name="Times New Roman Regular" size="11" fill="#000000"></TextSymbolizer>
        </Rule>
    </Style>
    <Style name="states_pg_styles">
        <Rule>
            <Filter>(([persons]&gt;2000000) and ([persons]&lt;4000000))</Filter>
            <PolygonSymbolizer>
                <CssParameter name="fill">#ff4d4d</CssParameter>
            </PolygonSymbolizer>
        </Rule>
        <Rule>
            <Filter>([persons]&gt;4000000)</Filter>
            <PolygonSymbolizer>
                <CssParameter name="fill">#4d4dff</CssParameter>
            </PolygonSymbolizer>
        </Rule>
        <Rule>
            <Filter>([persons]&lt;2000000)</Filter>
            <PolygonSymbolizer>
                <CssParameter name="fill">#4dff4d</CssParameter>
            </PolygonSymbolizer>
        </Rule>
    </Style>
    <Style name="states_shp_labels">
        <Rule>
            <TextSymbolizer name="STATE_ABBR" face_name="Times New Roman Regular" size="11" fill="#000000"></TextSymbolizer>
        </Rule>
    </Style>
    <Style name="states_shp_styles">
        <Rule>
            <Filter>(([PERSONS]&gt;2000000) and ([PERSONS]&lt;4000000))</Filter>
            <PolygonSymbolizer>
                <CssParameter name="fill">#ff4d4d</CssParameter>
            </PolygonSymbolizer>
        </Rule>
        <Rule>
            <Filter>([PERSONS]&gt;4000000)</Filter>
            <PolygonSymbolizer>
                <CssParameter name="fill">#4d4dff</CssParameter>
            </PolygonSymbolizer>
        </Rule>
        <Rule>
            <Filter>([PERSONS]&lt;2000000)</Filter>
            <PolygonSymbolizer>
                <CssParameter name="fill">#4dff4d</CssParameter>
            </PolygonSymbolizer>
        </Rule>
    </Style>
    <Style name="tiger_styles">
        <Rule>
            <LineSymbolizer>
                <CssParameter name="stroke-width">0.3</CssParameter>
            </LineSymbolizer>
        </Rule>
    </Style>
    <Layer name="states" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" status="0" clear_label_cache="0">
        <StyleName>states_shp_styles</StyleName>
        <StyleName>states_outlines</StyleName>
        <StyleName>states_shp_labels</StyleName>
        <Datasource>
            <Parameter name="file">../data/states</Parameter>
            <Parameter name="type">shape</Parameter>
        </Datasource>
    </Layer>
    <Layer name="states_pg" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" status="0" clear_label_cache="0">
        <StyleName>states_pg_styles</StyleName>
        <StyleName>states_outlines</StyleName>
        <StyleName>states_pg_labels</StyleName>
        <Datasource>
            <Parameter name="dbname">foss4g2008</Parameter>
            <Parameter name="host">localhost</Parameter>
            <Parameter name="password"></Parameter>
            <Parameter name="port">5432</Parameter>
            <Parameter name="table">states_pg</Parameter>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="user">postgres</Parameter>
        </Datasource>
    </Layer>
    <Layer name="tiger_shp" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" status="0" clear_label_cache="0">
        <StyleName>tiger_styles</StyleName>
        <Datasource>
            <Parameter name="file">../data/tiger_shp</Parameter>
            <Parameter name="type">shape</Parameter>
        </Datasource>
    </Layer>
    <Layer name="tiger_pg" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" status="0" clear_label_cache="0">
        <StyleName>tiger_styles</StyleName>
        <Datasource>
            <Parameter name="dbname">foss4g2008</Parameter>
            <Parameter name="host">localhost</Parameter>
            <Parameter name="password"></Parameter>
            <Parameter name="port">5432</Parameter>
            <Parameter name="table">tiger_pg</Parameter>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="user">postgres</Parameter>
        </Datasource>
    </Layer>
    <Layer name="merano" srs="+proj=utm +zone=32 +ellps=WGS84 +datum=WGS84 +units=m +no_defs" status="0" clear_label_cache="0">
        <StyleName>raster_style</StyleName>
        <Datasource>
            <Parameter name="file">../data/merano.tiff</Parameter>
            <Parameter name="type">gdal</Parameter>
        </Datasource>
    </Layer>
    <Layer name="merano2" srs="+proj=utm +zone=32 +ellps=WGS84 +datum=WGS84 +units=m +no_defs" status="0" clear_label_cache="0">
        <StyleName>raster_style</StyleName>
        <Datasource>
            <Parameter name="file">../data/merano_jptov.tiff</Parameter>
            <Parameter name="type">gdal</Parameter>
        </Datasource>
    </Layer>
    <Layer name="n_sid" srs="+proj=utm +zone=32 +ellps=WGS84 +datum=WGS84 +units=m +no_defs" status="0" clear_label_cache="0">
        <StyleName>raster_style</StyleName>
        <Datasource>
            <Parameter name="file">../data/S-34-30_2000.sid</Parameter>
            <Parameter name="type">gdal</Parameter>
        </Datasource>
    </Layer>
    <Layer name="s_sid" srs="+proj=utm +zone=34 +ellps=WGS84 +datum=WGS84 +units=m +no_defs" status="0" clear_label_cache="0">
        <StyleName>raster_style</StyleName>
        <Datasource>
            <Parameter name="file">../data/S-34-30_2000.sid</Parameter>
            <Parameter name="type">gdal</Parameter>
        </Datasource>
    </Layer>
</Map>
