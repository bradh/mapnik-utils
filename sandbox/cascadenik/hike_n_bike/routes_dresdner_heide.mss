.route.hiking_heide name
{
    shield-face-name: "DejaVu Sans Book";
    shield-size: 0;
    shield-fill: #070;
}

.route.hiking_heide[zoom>=12]
{
    shield-min-distance: 2;
    shield-spacing: 22;
}
.route.hiking_heide[zoom>=14]
{
    shield-min-distance: 5;
    shield-spacing: 55;
}
.route.hiking_heide[zoom>=15]
{
    shield-min-distance: 8;
    shield-spacing: 88;
}


