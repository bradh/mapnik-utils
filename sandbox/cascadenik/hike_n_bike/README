sed --file=/opt/applications/tilesathome/mapnik-utils-sandbox/cascadenik/hike_n_bike/replacements.sed /home/colin/mapnik/osm_teczno_customized.xml | sponge /home/colin/mapnik/osm_teczno_customized.xml


